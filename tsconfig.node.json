{
	"compilerOptions": {
		"composite": true,
		"tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",
		"skipLibCheck": true,
		"module": "ESNext",
		"moduleResolution": "bundler",
		"allowSyntheticDefaultImports": true,
		"strict": false,
		"noEmit": true,
		"paths": {
			"~*": ["src*"]
		},
		"baseUrl": "."
	},
	"include": ["vite.config.ts", "vite.*.config.ts"]
}
