{
	"manifest_version": 3,
	"version": "0.0.1",
	"email": "jinli.regret@gmail.com",
	"name": "DEV | Agents Viewer(Provide professional, vertical agent services)",
	"description": "Using side-panel API in an extension",
	"key": "",
	"icons": {
		"16": "assets/16x16.png",
		"32": "assets/32x32.png",
		"48": "assets/48x48.png",
		"64": "assets/64x64.png",
		"128": "assets/128x128.png"
	},
	"action": {
		"title": "Agents Viewer",
		"default_icon": {
			"16": "assets/16x16.png",
			"32": "assets/32x32.png",
			"48": "assets/48x48.png",
			"64": "assets/64x64.png",
			"128": "assets/128x128.png"
		}
	},
	"options_page": "options.html",
	"background": {
		"service_worker": "./src/crx/service-worker.ts"
	},
	"content_scripts": [
		{
			"matches": ["http://*/*", "https://developer.mozilla.org/*"],
			"js": ["./src/crx/content.tsx"]
		}
	],
	"side_panel": { "default_path": "index.html" },
	"permissions": ["sidePanel", "identity", "storage"],
	"host_permissions": ["https://*/*"]
}
